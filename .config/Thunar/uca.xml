<action>
	<icon></icon>
	<name>Creates a symbolic link in the current directory.</name>
	<submenu></submenu>
	<unique-id>1720980552739238-2</unique-id>
	<command>/home/jackmortimer/.config/Thunar/custom_actions/symlink.sh %f</command>
	<description>Creates a symbolic link in the current directory.</description>
	<range></range>
	<patterns>*</patterns>
	<directories/>
	<audio-files/>
	<image-files/>
	<other-files/>
