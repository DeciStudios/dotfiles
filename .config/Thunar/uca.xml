<action>
	<icon></icon>
	<name>Open in neovide</name>
	<submenu></submenu>
	<unique-id>1721076199233553-1</unique-id>
	<command>bash -c &apos;if [ -d &quot;%f&quot; ]; then cd &quot;%f&quot; &amp;&amp; neovide .; else neovide &quot;%f&quot;; fi&apos;
</command>
	<description>Opens the file/folder in Neovide</description>
	<range>*</range>
	<patterns>*</patterns>
	<directories/>
	<other-files/>
	<text-files/>
</action>
<action>
	<icon></icon>
	<name>Open in nvim</name>
	<submenu></submenu>
	<unique-id>1721076222624474-2</unique-id>
	<command>wezterm start -- bash -c &apos;if [ -d &quot;%f&quot; ]; then cd &quot;%f&quot;; else cd &quot;$(dirname &quot;%f&quot;)&quot;; fi &amp;&amp; nvim &quot;%f&quot;&apos;
</command>
	<description>Opens the file/folder in Neovim</description>
	<range>*</range>
	<patterns>*</patterns>
	<directories/>
	<other-files/>
	<text-files/>
</action>
